<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
	  <PackageIcon>cybtan.png</PackageIcon>
	  <RepositoryUrl>https://github.com/ansel86castro/cybtans-sdk</RepositoryUrl>
	  <PackageTags>automation ,selenium</PackageTags>
	  <Authors>Ansel Castro</Authors>
	  <Product>Cybtans SDK</Product>
	  <Version>4.1.0</Version>
	  <Description>Selenium Automation Framework</Description>
  </PropertyGroup>

	<ItemGroup>		
		<PackageReference Include="Selenium.Support" Version="4.21.0" />
		<PackageReference Include="Selenium.WebDriver" Version="4.21.0" />
		<PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="125.0.6422.6000" />
		<PackageReference Include="Selenium.WebDriver.GeckoDriver" Version="0.34.0" />
		<PackageReference Include="Selenium.WebDriver.MicrosoftDriver" Version="17.17134.0" />		
	</ItemGroup>

	<ItemGroup>
		<None Include="..\cybtan.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
	
</Project>
